    "test": "clear&&ng test",
    "lint": "ng lint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org sentry --project pixlise ./dist/pixlise && sentry-cli --url https://3.90.230.26 sourcemaps upload --org sentry --project pixlise ./dist/pixlise"
  },
